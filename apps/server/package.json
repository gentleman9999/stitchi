{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/server",
    "dev:typecheck": "tsc --watch --noEmit",
    "start": "node dist/src/server",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "build": "yarn clean && yarn codegen:prisma && tsc -b",
    "build:docker": "cd ../.. && docker build -t stitchi-server .",
    "codegen": "yarn codegen:prisma && yarn codegen:catalog",
    "codegen:prisma": "prisma generate",
    "codegen:catalog": "node ./scripts/generate-catalog.mjs",
    "db:migrate": "npx prisma db push",
    "db:inspect": "npx prisma studio",
    "stripe:listen": "stripe listen --forward-to localhost:5001/api/v1/stripe/webhook",
    "email:compose": "email dev --dir src/email-template-composer/emails --port 3004"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "18.17.0",
    "@types/node-fetch": "^2.6.1",
    "@types/pino": "^7.0.5",
    "@types/react": "^18.2.33",
    "@types/uuid": "^9.0.1",
    "@types/ws": "^8.5.5",
    "config": "*",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "openai": "^3.2.1",
    "pino-pretty": "^10.3.1",
    "prisma": "5",
    "react-email": "^1.9.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^1.1.8",
    "tsconfig": "*",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@axiomhq/pino": "^0.1.3",
    "@devoxa/prisma-relay-cursor-connection": "^3.1.0",
    "@graphql-tools/stitch": "^8.6.2",
    "@graphql-tools/utils": "^8.6.5",
    "@graphql-tools/wrap": "^8.4.9",
    "@opentelemetry/api": "^1.4.1",
    "@opentelemetry/auto-instrumentations-node": "^0.39.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.41.2",
    "@opentelemetry/sdk-node": "^0.41.2",
    "@prisma/client": "5",
    "@react-email/components": "^0.0.7",
    "@react-email/render": "^0.0.7",
    "@sendgrid/client": "^7.7.0",
    "apollo-server": "^3.12.1",
    "apollo-server-core": "^3.12.1",
    "auth0": "^4.1.0",
    "chalk": "^5.2.0",
    "crypto": "^1.0.1",
    "csv-parser": "^3.0.0",
    "currency.js": "^2.0.4",
    "date-fns": "^2.30.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.6.0",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.15.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.14.0",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^3.1.0",
    "langchain": "^0.1.5",
    "mathjs": "^12.4.0",
    "nexus": "^1.3.0",
    "pino": "^8.19.0",
    "quote": "*",
    "slugify": "^1.6.5",
    "stripe": "^12.6.0",
    "tailwindcss": "^3.2.7",
    "uuid": "^9.0.0",
    "ws": "^8.13.0",
    "yup": "^1.0.2",
    "zod": "^3.22.4"
  }
}
