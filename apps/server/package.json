{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/server",
    "dev:typecheck": "tsc --watch --noEmit",
    "start": "node dist/src/server",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "build": "yarn clean && yarn codegen && tsc",
    "build:docker": "cd ../.. && docker build -t stitchi-server .",
    "codegen": "yarn codegen:prisma",
    "codegen:prisma": "prisma generate",
    "db:migrate": "npx prisma db push",
    "db:inspect": "npx prisma studio",
    "stripe:listen": "stripe listen --forward-to localhost:5000/api/v1/stripe/webhook"
  },
  "devDependencies": {
    "@types/auth0": "^2.34.13",
    "@types/graphql-relay": "^0.7.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.21",
    "@types/node-fetch": "^2.6.1",
    "config": "*",
    "openai": "^3.2.1",
    "prisma": "^4.14.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^1.1.8",
    "tsconfig": "*",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@graphql-tools/stitch": "^8.6.2",
    "@graphql-tools/utils": "^8.6.5",
    "@graphql-tools/wrap": "^8.4.9",
    "@prisma/client": "^4.15.0",
    "@sendgrid/client": "^7.7.0",
    "@types/uuid": "^9.0.1",
    "apollo-server": "^3.6.6",
    "auth0": "^2.40.0",
    "chalk": "^5.2.0",
    "cloudinary": "^1.28.1",
    "csv-parser": "^3.0.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.0.0",
    "graphql": "^16.3.0",
    "graphql-relay": "^0.10.0",
    "graphql-scalars": "^1.15.0",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.5",
    "langchain": "^0.0.73",
    "mathjs": "^11.8.0",
    "nexus": "^1.3.0",
    "slugify": "^1.6.5",
    "stripe": "^12.6.0",
    "uuid": "^9.0.0",
    "yup": "^1.0.2"
  }
}
