{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/server",
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "build": "yarn clean && yarn codegen && tsc",
    "codegen": "yarn codegen:prisma && yarn codegen:nexus",
    "codegen:prisma": "prisma generate",
    "codegen:nexus": "ts-node --transpile-only src/schema"
  },
  "devDependencies": {
    "@types/auth0": "^2.34.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.21",
    "prisma": "^3.10.0",
    "ts-node": "^10.6.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig": "*",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@prisma/client": "^3.10.0",
    "apollo-server-core": "^3.6.4",
    "apollo-server-express": "^3.6.4",
    "auth0": "^2.40.0",
    "express": "^4.17.3",
    "graphql-scalars": "^1.15.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.5",
    "nexus": "^1.3.0"
  }
}
