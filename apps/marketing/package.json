    "icons": "*",
    "lodash-es": "^4.17.21",
    "next": "latest",
    "next-seo": "^5.1.0",
    "react": "^17.0.2",
    "react-datocms": "^2.0.1",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.27.0",
