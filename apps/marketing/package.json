{
  "name": "marketing",
  "version": "0.0.0",
  "private": true,
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "dev": "next dev --port 3000",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "eslint src --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf .next",
    "codegen": "yarn codegen:graphql && yarn codegen:seo && yarn codegen:static",
    "codegen:graphql": "npx apollo client:codegen ./src/generated --target typescript --outputFlat",
    "codegen:seo": "node ./scripts/generate-seo.mjs",
    "codegen:static": "node ./scripts/generate-static.mjs",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build"
  },
  "dependencies": {
    "@apollo/client": "^3.7.10",
    "@headlessui/react": "^1.7.8",
    "@hookform/resolvers": "^3.0.0",
    "@next/bundle-analyzer": "^13.2.4",
    "@radix-ui/react-dialog": "^1.0.3",
    "@radix-ui/react-popover": "^1.0.5",
    "@radix-ui/react-slider": "^1.0.0",
    "@radix-ui/react-switch": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.0.5",
    "@sendgrid/client": "^7.6.1",
    "classnames": "^2.3.1",
    "copy-to-clipboard": "^3.3.3",
    "currency.js": "^2.0.4",
    "datocms-structured-text-to-plain-text": "^2.0.4",
    "datocms-structured-text-utils": "^2.0.4",
    "dayjs": "^1.10.7",
    "framer-motion": "^8.0.2",
    "google-auth-library": "^8.7.0",
    "googleapis": "^118.0.0",
    "hooks": "*",
    "icons": "*",
    "lodash-es": "^4.17.21",
    "next": "^13.2.4",
    "next-seo": "^5.1.0",
    "next-sitemap": "^4.0.6",
    "next-usequerystate": "^1.7.2",
    "nextjs-progressbar": "^0.0.16",
    "pluralize": "^8.0.0",
    "react": "^18.2.0",
    "react-datocms": "^4.0.9",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-loading-skeleton": "^3.2.0",
    "react-merge-refs": "^1.1.0",
    "sharp": "^0.31.3",
    "typewriter-effect": "^2.18.2",
    "uuid": "^9.0.0",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.21",
    "@types/pluralize": "^0.0.29",
    "@types/react": "^18.0.21",
    "@types/uuid": "^9.0.1",
    "@types/yup": "^0.29.13",
    "autoprefixer": "10.4.14",
    "config": "*",
    "cross-env": "^7.0.3",
    "eslint": "^8.6.0",
    "tailwindcss": "^3.2.7",
    "tsconfig": "*",
    "typescript": "^4.9.5"
  }
}
